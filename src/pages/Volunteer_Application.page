                                            </tr>
                                            <tr>
                                                <td><apex:inputCheckbox value="{!InstrumentTeacherGuitar}"/></td>
                                                <td><span>Guitar</span></td>
                                            </tr>
                                            <tr>
                                                <td><apex:inputCheckbox value="{!InstrumentTeacherKeyboards}"/></td>
