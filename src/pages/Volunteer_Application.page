                            <apex:pageBlockSectionItem  >
                                <apex:outputLabel styleClass="musicalExperienceExplained hideRow" value="Please explain your previous musical experience:"/>
                                <apex:inputField styleClass="musicalExperienceExplainedText" value="{!newApp.Musical_Experience_Explained__c}"/>
                            </apex:pageBlockSectionItem>
                            <apex:pageblockSectionItem >
                                <apex:outputLabel value="If you checked INSTRUMENT TEACHER above, please indicate which instrument(s) you feel competent teaching." styleClass="question instrument-teacher hidden"/>
                                <apex:outputPanel styleClass="instrument-teacher hidden">
                                    <apex:outputLabel styleClass="wideLabel italic" value="Check all that apply." />
                                    <table>
                                        <tbody>
