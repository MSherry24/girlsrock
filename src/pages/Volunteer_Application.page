                            </apex:pageblockSectionItem>
                            <apex:pageBlockSectionItem>
                                <apex:outputPanel styleClass="JuniorCounsellorQuestion" style="display:none">
                                    <apex:outputLabel value="If applying for JUNIOR COUNSELOR, please select which describes you:" styleClass="question"/>
                                </apex:outputPanel>
                                <apex:outputPanel styleClass="JuniorCounsellorQuestion" style="display:none">
                                    <apex:inputField id="juniorCounsellor" value="{!newApp.Junior_Counsellor__c}" required="false"/>
                                </apex:outputPanel>
                            </apex:pageBlockSectionItem>
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel value="Do you have any experience fixing or working with GEAR (guitars, drums, bass, amps, etc.)"/>
                                <apex:inputField styleClass="gearExperienceInput" value="{!newApp.Gear_Experience__c}" onChange="showHideSectionByPicklistValue('gearExperienceInput', 'gearExperienceExplained', ['Yes', 'Some'])"/>
                            </apex:pageBlockSectionItem>
                            <apex:pageBlockSectionItem>
                                <apex:outputLabel styleClass="gearExperienceExplained hideRow" value="Please explain your previous gear experience:"/>
                                <apex:inputField styleClass="gearExperienceExplainedText" value="{!newApp.Gear_Experience_Explained__c}"/>
                            </apex:pageBlockSectionItem>
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel value="Do you have any musical experience? "/>
                                <apex:inputField styleClass="musicalExperienceInput" value="{!newApp.Musical_Experience__c}" onChange="showHideSectionByPicklistValue('musicalExperienceInput', 'musicalExperienceExplained', ['Yes', 'Some'])"/>
