                                </apex:outputPanel>
                            </apex:pageBlockSectionItem>
                            <apex:pageBlockSectionItem>
                                <apex:outputLabel value="Address Line 2" styleClass="question"/>
                                <apex:inputField id="addressLine2" value="{!newApp.Address_Line_2__c}" required="false"/>
                            </apex:pageBlockSectionItem>
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel value="City" styleClass="question"/>
                                <apex:inputField id="city" value="{!newApp.City__c}" required="{!validationOn}"/>
                            </apex:pageBlockSectionItem>
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel value="State" styleClass="question"/>
                                <apex:inputField id="state" value="{!newApp.State__c}" required="{!validationOn}"/>
                            </apex:pageBlockSectionItem>
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel value="Zip" styleClass="question"/>
                                <apex:inputField id="zipCode" value="{!newApp.Zip_Code__c}" required="{!validationOn}"/>
                            </apex:pageBlockSectionItem>
                            <apex:pageBlockSectionItem >
                                <apex:outputLabel value="If you are coming from out of town, would you like help finding a Chicago host?" styleClass="question"/>
                                <apex:inputField id="comingFromOutOfTown" value="{!newApp.Coming_From_Out_Of_Town__c}" required="false"/>
                            </apex:pageBlockSectionItem>
                        </apex:pageBlockSection>
                    </apex:outputPanel>
                    <!-- Page 2 - AVAILABILITY -->
